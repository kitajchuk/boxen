{.section item}
    <div class="starred -expt">
        <div class="starred__link">
            <img class="image js-lazy-image" data-img-src="{assetUrl}" data-variants="{systemDataVariants}" data-original-size="{originalSize}" />
        </div>
        <div class="starred__info">
            <div class="starred__title" data-content-field="title">
                {title}
                {.if body}
                    <div class="p teal">{body}</div>
                {.end}
            </div>
            <div class="starred__meta">
                {.repeated section tags}<span class="tag"><a href="{collection.fullUrl}?tag={@}">{@}</a></span>{.alternates with} {.end}
            </div>
        </div>
    </div>
{.end}
