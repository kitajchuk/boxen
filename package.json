{
  "name": "boxen",
  "version": "0.2.0",
  "description": "A content-driven Squarespace developer SDK",
  "author": "Brandon Lee Kitajchuk",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "postinstall": "node ./boxen.install",
    "start": "npm run sqs:setup && (npm run watch & npm run sqs:server:auth)",
    "build": "npm run sqs:assemble ; npm run boxen:scaffold ; npm run boxen:build",
    "watch": "npm run sqs:assemble:watch & ./node_modules/.bin/webpack --env.sandbox --hide-modules --colors --watch",
    "boxen:build": "./node_modules/.bin/webpack --env.production --progress --profile --display-modules --colors -p",
    "boxen:scaffold": "node ./boxen.scaffold",
    "sqs:setup": "./node_modules/.bin/squarespace setup",
    "sqs:assemble": "./node_modules/.bin/squarespace assemble",
    "sqs:assemble:watch": "./node_modules/.bin/squarespace assemble --watch --trigger-reload",
    "sqs:server": "./node_modules/.bin/squarespace runserver --trigger-reload --verbose --faster-less",
    "sqs:server:auth": "./node_modules/.bin/squarespace runserver --auth --trigger-reload --verbose --faster-less",
    "sqs:deploy": "npm run build && npm run sqs:setup && ./node_modules/.bin/squarespace deploy"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:kitajchuk/boxen.git"
  },
  "dependencies": {
    "@squarespace/core": "^1.1.0"
  },
  "devDependencies": {
    "@squarespace/toolbelt": "^0.10.3",
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.4",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "expose-loader": "^0.7.5",
    "fg-loadjs": "^1.0.0",
    "file-loader": "^1.1.11",
    "node-sass": "^4.13.0",
    "on-build-webpack": "^0.1.0",
    "open": "0.0.5",
    "paramalama": "~0.2",
    "postcss": "^6.0.22",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-preset-env": "^5.1.0",
    "properjs-blit": "^0.2.0",
    "properjs-controller": "~0.2",
    "properjs-debounce": "^0.2.0",
    "properjs-easing": "~0.2",
    "properjs-hobo": "^0.3.26",
    "properjs-imageloader": ">=0.3.1",
    "properjs-jscroll": "^0.1.0",
    "properjs-lager": "^0.1.2",
    "properjs-mediabox": "~0.3",
    "properjs-pagecontroller": "~0.4",
    "properjs-resizecontroller": "~0.2",
    "properjs-scroll2": "~0.2",
    "properjs-scrollcontroller": "~0.2",
    "properjs-template": "~0.2",
    "properjs-throttle": "^0.3.0",
    "properjs-tween": "~0.2",
    "request": "^2.83.0",
    "sass-loader": "^7.0.3",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4"
  }
}
