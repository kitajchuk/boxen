{
  "name": "boxen",
  "version": "0.2.0",
  "description": "A content-driven Squarespace developer SDK",
  "author": "Brandon Lee Kitajchuk",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "setup": "./node_modules/.bin/squarespace setup",
    "start": "npm run setup && (npm run watch & npm run server:auth)",
    "postinstall": "node ./boxen.install",
    "build": "npm run assemble ; npm run boxen:scaffold ; npm run webpack",
    "boxen:scaffold": "node ./boxen.scaffold",
    "assemble": "./node_modules/.bin/squarespace assemble",
    "assemble:watch": "./node_modules/.bin/squarespace assemble --watch --trigger-reload",
    "webpack": "./node_modules/.bin/webpack --env.production --progress --profile --display-modules --colors -p",
    "webpack:watch": "./node_modules/.bin/webpack --env.sandbox --hide-modules --colors --watch",
    "watch": "npm run assemble:watch & npm run webpack:watch",
    "server": "./node_modules/.bin/squarespace runserver --trigger-reload --verbose --faster-less",
    "server:auth": "./node_modules/.bin/squarespace runserver --auth --trigger-reload --verbose --faster-less",
    "deploy": "npm run build && npm run setup && ./node_modules/.bin/squarespace deploy"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:kitajchuk/boxen.git"
  },
  "dependencies": {
    "@squarespace/core": "^1.1.0",
    "gsap": "^2.0.1"
  },
  "devDependencies": {
    "@squarespace/toolbelt": "^0.8.4",
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "expose-loader": "^0.7.5",
    "fg-loadjs": "^1.0.0",
    "file-loader": "^1.1.11",
    "node-sass": "^4.9.0",
    "on-build-webpack": "^0.1.0",
    "open": "0.0.5",
    "paramalama": "~0.2",
    "postcss": "^6.0.22",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-preset-env": "^5.1.0",
    "properjs-controller": "~0.2",
    "properjs-debounce": "^0.2.0",
    "properjs-easing": "~0.2",
    "properjs-hobo": "^0.3.26",
    "properjs-imageloader": ">=0.3.1",
    "properjs-lager": "^0.1.2",
    "properjs-mediabox": "~0.3",
    "properjs-pagecontroller": "~0.4",
    "properjs-resizecontroller": "~0.2",
    "properjs-scroll2": "~0.2",
    "properjs-scrollcontroller": "~0.2",
    "properjs-template": "~0.2",
    "properjs-throttle": "^0.3.0",
    "properjs-tween": "~0.2",
    "request": "^2.83.0",
    "sass-loader": "^7.0.3",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4"
  }
}
