{
  "name": "boxen",
  "version": "0.2.0",
  "description": "A content-driven Squarespace developer SDK",
  "author": "Brandon Lee Kitajchuk",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "postinstall": "node ./boxen.install",
    "start": "npm run sqs:setup",
    "poststart": "(npm run watch & npm run sqs:server:auth)",
    "build": "npm run sqs:assemble ; npm run boxen:scaffold ; npm run boxen:build",
    "watch": "npm run sqs:assemble:watch & ./node_modules/.bin/webpack --env.sandbox --hide-modules --colors --watch",
    "boxen:build": "./node_modules/.bin/webpack --env.production --progress --profile --display-modules --colors -p",
    "boxen:scaffold": "node ./boxen.scaffold",
    "sqs:setup": "./node_modules/.bin/squarespace setup",
    "sqs:assemble": "./node_modules/.bin/squarespace assemble",
    "sqs:assemble:watch": "./node_modules/.bin/squarespace assemble --watch --trigger-reload",
    "sqs:server": "./node_modules/.bin/squarespace runserver --trigger-reload --verbose --faster-less",
    "sqs:server:auth": "./node_modules/.bin/squarespace runserver --auth --trigger-reload --verbose --faster-less",
    "sqs:deploy": "npm run build && npm run sqs:setup && ./node_modules/.bin/squarespace deploy"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:kitajchuk/boxen.git"
  },
  "dependencies": {
    "@squarespace/core": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@squarespace/toolbelt": "^0.10.6",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.20.0",
    "eslint-webpack-plugin": "^2.5.1",
    "expose-loader": "^0.7.5",
    "fg-loadjs": "^1.0.0",
    "file-loader": "^6.0.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^7.0.0",
    "open": "^7.4.0",
    "paramalama": "^0.3.0",
    "properjs-blit": "^0.3.2",
    "properjs-controller": "^0.3.1",
    "properjs-debounce": "^0.3.0",
    "properjs-hobo": "^0.3.26",
    "properjs-imageloader": "^0.4.0",
    "properjs-lager": "^0.1.2",
    "properjs-pagecontroller": "^0.5.0",
    "properjs-resizecontroller": "^0.3.0",
    "properjs-scrollcontroller": "^0.3.0",
    "sass-loader": "^8.0.2",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^5.21.2",
    "webpack-cli": "^3.3.11"
  }
}
